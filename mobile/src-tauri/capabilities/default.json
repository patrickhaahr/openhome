{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    {
      "identifier": "http:default",
      "allow": [{ "url": "http://localhost:8000/*" }, { "url": "https://api.openhome.local/*" }, { "url": "http://192.168.8.222:8000/*" }],
      "deny": []
    },
    "store:default"
  ]
}
